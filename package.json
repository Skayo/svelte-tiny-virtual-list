	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@playwright/test": "1.41.2",
		"@sveltejs/adapter-auto": "3.1.1",
		"@sveltejs/kit": "2.5.0",
		"@sveltejs/package": "2.2.6",
		"@sveltejs/vite-plugin-svelte": "3.0.2",
		"@types/eslint": "8.56.0",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.35.1",
